{
  "name": "easyjet-test",
  "version": "1.0.0",
  "description": "Test for easyJet",
  "main": "main.js",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "watchify -v -d -t [ reactify --es6 ] main.js -o compiled.js",
    "build": "NODE_ENV=production browserify -t [ reactify --es6 ] main.js | uglifyjs > compiled.js | lessc assets/less/styles.less assets/css/styles.css | lessc --clean-css assets/less/styles.less assets/css/styles.min.css"
  },
  "author": "Alex Whitlock",
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.2.1",
    "react-dom": "*"
  },
  "devDependencies": {
    "browserify": "^9.0.8",
    "reactify": "^1.1.0",
    "uglify-js": "^2.4.20",
    "watchify": "^3.1.2",
    "less": "*",
    "less-plugin-clean-css": "*"
  }
}
